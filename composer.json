{
    "name": "kielabokkie/laravel-conceal",
    "description": "Conceal data in an array or collection",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "kielabokkie",
            "email": "kielabokkie@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "^5.7|^6.0|^7.0|^8.0|^9.0",
        "php": "^7.2|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.7|^4.0"
    },
    "autoload": {
        "psr-4": {
            "Kielabokkie\\LaravelConceal\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Kielabokkie\\LaravelConceal\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kielabokkie\\LaravelConceal\\ConcealServiceProvider"
            ],
            "aliases": {
                "Concealer": "Kielabokkie\\LaravelConceal\\Facades\\Concealer"
            }
        }
    }
}
